<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:budiga_app.MVVM.View"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="budiga_app.MVVM.View.OverviewView"
      mc:Ignorable="d" 
      d:DesignHeight="720" Width="1020"
      Title="OverviewView">

    <Grid>
        <StackPanel Height="460" Margin="0,260,0,0" >
            <materialDesign:Card Height="441" Margin="10">
                <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" d:Name="OverviewTable"/>
            </materialDesign:Card>

        </StackPanel>
    </Grid>
</Page>
